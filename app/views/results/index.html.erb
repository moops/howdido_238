<h1>Listing results</h1>

<table>
  <tr>
    <th>Athlete</th>
    <th>Race</th>
    <th>Overall place</th>
    <th>Bib</th>
    <th>Div</th>
    <th>Div place</th>
  </tr>

<% @results.each do |result| %>
  <tr>
    <td><%=h result.athlete_id %></td>
    <td><%=h result.race_id %></td>
    <td><%=h result.overall_place %></td>
    <td><%=h result.bib %></td>
    <td><%=h result.div %></td>
    <td><%=h result.div_place %></td>
    <td><%= link_to 'Show', result %></td>
    <td><%= link_to 'Edit', edit_result_path(result) %></td>
    <td><%= link_to 'Destroy', result, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New result', new_result_path %>